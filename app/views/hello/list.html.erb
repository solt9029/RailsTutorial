<table>
    <tr>
        <th>isbn</th>
        <th>title</th>
        <th>price</th>
        <th>publish</th>
        <th>published</th>
        <th>dl</th>
    </tr>

    <% @books.each do |book| %>
    <tr>
        <td><%= book.isbn %></td>
        <td><%= book.title %></td>
        <td><%= book.price %></td>
        <td><%= book.publish %></td>
        <td><%= book.published %></td>
        <td><%= book.dl %></td>
    </tr>
    <% end %>
</table>